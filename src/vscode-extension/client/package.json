{
  "name": "workflow-vscode-client",
  "version": "0.1.0",
  "description": "VSCode extension client for Workflow YAML files",
  "main": "./dist/extension.js",
  "scripts": {
    "build": "webpack --config webpack.extension.config.js --mode production && webpack --config webpack.config.js --mode production",
    "type-check": "tsc --noEmit",
    "watch": "webpack --config webpack.extension.config.js --mode development --watch & webpack --config webpack.config.js --mode development --watch",
    "clean": "rm -rf dist",
    "compile": "npm run build"
  },
  "dependencies": {
    "@workflow/graph": "workspace:*",
    "@workflow/types": "workspace:*",
    "@workflow/validation": "workspace:*",
    "@xyflow/react": "^12.0.0",
    "dagre": "^0.8.5",
    "html-to-image": "^1.11.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vscode-languageclient": "^9.0.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/dagre": "^0.7.52",
    "@types/node": "^20.19.25",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/vscode": "^1.95.0",
    "css-loader": "^7.1.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.9.3",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "vscode": "^1.95.0"
  }
}
